{
  "compilerOptions": {
    "target": "ES2018",
    "module": "CommonJs",
    "allowJs": true,
    "jsx": "preserve",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}